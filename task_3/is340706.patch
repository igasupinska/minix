+			${ECHO} "The following files should be created for ${PKGNAME}:"
+			;;
+		esac
+		${ECHO} ""
+		case $f_mode:$f_user:$f_group in
+		::)
+			${ECHO} "	$file"
+			;;
+		[!:]*::)
+			${ECHO} "	$file (m=$f_mode)"
+			;;
+		[!:]*:[!:]*:)
+			${ECHO} "	$file (m=$f_mode, o=$f_user)"
+			;;
+		[!:]*:[!:]*:[!:]*)
+			${ECHO} "	$file (m=$f_mode, o=$f_user, g=$f_group)"
+			;;
+		esac
