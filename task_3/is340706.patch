diff -rupN czysty/usr/src/lib/libc/misc/distort_time.c moj/usr/src/lib/libc/misc/distort_time.c
--- czysty/usr/src/lib/libc/misc/distort_time.c	1970-01-01 01:00:00.000000000 +0100
+++ moj/usr/src/lib/libc/misc/distort_time.c	2019-04-30 17:54:55.000000000 +0200
@@ -0,0 +1,24 @@
+#include <lib.h>
+#include <stdio.h>
+#include <stdlib.h>
+#include <sys/types.h>
+#include <unistd.h>
+#include <minix/rs.h>
+
+int distort_time(pid_t pid, uint8_t scale)
+{
+	endpoint_t pm_ep;
+	message m;
+	m.m_m1.m1i1 = pid;
+	m.m_m1.m1i2 = scale;
+
+	if (minix_rs_lookup("pm", &pm_ep) != 0)
+	{
+		errno = ENOSYS;
+		return -1;
+	}
+	if (_syscall(pm_ep, PM_DISTORT_TIME, &m) < 0) {
+		return errno;
+	}
+	return 0;
+}
